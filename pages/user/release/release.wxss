/* pages/user/release/release.wxss */

.server{
  width: 100%;
  text-align: center;
  position: relative;
}
.nodata{
  width: 64px;
  margin: 20px auto 0;
}
.img{
  width: 100%;
  height: 64px;
}
.text{
  color: #999;
}
.server .text:after,.server .text:before {
  background: #eee;
  content: "";
  height: 1px;
  position: absolute;
  top: 50%;
  width: 20%;}  
    /*调整背景横线的左右距离*/  
.server .text:before {
  left: 15%;
}
.server .text:after {
  right: 15%;
}  